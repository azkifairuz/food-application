    <RelativeLayout
    android:id="@+id/headerTransaksi"

    android:layout_width="match_parent"
    android:layout_height="185dp"
    android:background="@drawable/radiusnavbar">

    <LinearLayout

        android:id="@+id/layout_title"
        android:layout_marginTop="40dp"
        android:layout_marginHorizontal="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center">

        <View
            android:id="@+id/back_button"
            android:layout_width="6.75dp"
            android:layout_height="13dp"
            android:backgroundTint="@color/white"
            android:background="@drawable/backl_icon"/>
        <View
            android:layout_marginStart="75.24dp"
            android:layout_marginEnd="71.99dp"
            android:layout_width="130dp"
            android:layout_height="30dp"
            android:background="@drawable/name"/>
        <View
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/notification_icon"/>
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="55dp"
        android:layout_below="@id/layout_title"
        android:paddingHorizontal="15dp"
        android:layout_width="320dp"
        android:layout_height="34dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/shape_searchbar">
        <View
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/search_icon"
            android:layout_gravity="center_vertical"/>

        <EditText

            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:textSize="12sp"
            android:textAlignment="textStart"
            android:background="#ffffff"
            android:fontFamily="@font/poppins_regular"
            android:padding="8dp"
            android:hint="Cari Transaksi"/>
    </LinearLayout>

</RelativeLayout>
